.singer-list-item {
  display: flex;
  align-items: center;
  color: white
  font-size: 32rpx;
  height: 80px;
  .left {
    width: 80px;
    height: 100%;
    padding: 16px;
    box-sizing: border-box;
    image {
      width: 100%;
      height: 100%;
      border-radius: 50%;
    }
  }
  .right {
    flex: 1;
  }
}