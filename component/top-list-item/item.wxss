.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
page {
  background: #343434;
}
.under-top {
  margin-top: 90rpx;
}
scroll-view.common-scroll {
  position: fixed;
  top: 90rpx;
  left: 0;
  right: 0;
  bottom: 90rpx;
}
.left-right {
  display: flex;
  align-items: center;
}
.left-right .right {
  flex: 1;
  overflow: hidden;
}
.ellipse-single {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.top-item {
  display: flex;
  align-items: center;
  margin: 40rpx 20rpx;
}
.top-item .left {
  display: block;
  width: 100rpx;
  height: 100rpx;
  margin-right: 20rpx;
}
.top-item .left image {
  width: 100%;
  height: 100%;
}
.top-item .right {
  flex: 1;
  overflow: hidden;
  font-size: 30rpx;
}
